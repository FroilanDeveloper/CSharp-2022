@model Dish
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Detail</title>
</head>
<body>
  <a href="/">Home</a>
  <div>
    <h1>@Model.Name</h1>
  </div>
  <div>
    <h3>@Model.Chef</h3>
  </div>
  <hr>
  <h3>@Model.Description</h3>
  <h3>Calories: @Model.Calories</h3>
  <h3>Tastiness: @Model.Tastiness</h3>
  <a href="/delete/@Model.DishId"><button>Delete</button></a>
  <a href="/edit/@Model.DishId"><button>Edit</button></a>
</body>
</html>